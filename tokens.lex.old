%{
  #include "y.tab.h"
  #include <string.h>
%}

%%
[a-zA-Z_][a-zA-Z0-9_]*   { yylval = strdup(yytext); return IDENTIFIER; }
[0-9]+                   { yylval = atoi(yytext); return INTEGER; }
[+]                      { return ADD; }
[-]                      { return SUB; }
.                        { return yytext[0]; }
%%

int yywrap(void) {
    return 1;
}
